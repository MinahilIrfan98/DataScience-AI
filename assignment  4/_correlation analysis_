   "source": [
    "# ========== IMPORT LIBRARIES ==========\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# ========== LOAD DATA ==========\n",
    "df = pd.read_csv(r\"C:\\Users\\DELL\\Downloads\\twitter_sentiment_cleaned.csv\")\n",
    "df.columns = [\"target\", \"ids\", \"date\", \"flag\", \"user\", \"text\"]\n",
    "\n",
    "# Map sentiment labels\n",
    "df['sentiment'] = df['target'].map({0: 'Negative', 4: 'Positive'})\n",
    "\n",
    "# ========== FEATURE CREATION ==========\n",
    "# Convert text data into numeric features for correlation\n",
    "df['tweet_length'] = df['text'].str.len()             # number of characters\n",
    "df['word_count'] = df['text'].apply(lambda x: len(str(x).split()))  # number of words\n",
    "df['avg_word_length'] = df['tweet_length'] / df['word_count']       # average word size\n",
    "\n",
    "# Encode sentiment target as 0/1 for correlation\n",
    "df['target'] = df['sentiment'].map({'Negative': 0, 'Positive': 1})\n",
    "\n",
    "# Select only numeric features\n",
    "numeric_df = df[['target', 'tweet_length', 'word_count', 'avg_word_length']]\n",
    "\n",
    "# ========== DESCRIPTIVE STATISTICS ==========\n",
    "mean_values = numeric_df.mean()\n",
    "median_values = numeric_df.median()\n",
    "mode_values = numeric_df.mode().iloc[0]\n",
    "variance_values = numeric_df.var()\n",
    "\n",
    "print(\"===== DESCRIPTIVE STATISTICS =====\")\n",
    "print(\"Mean:\\n\", mean_values, \"\\n\")\n",
    "print(\"Median:\\n\", median_values, \"\\n\")\n",
    "print(\"Mode:\\n\", mode_values, \"\\n\")\n",
    "print(\"Variance:\\n\", variance_values, \"\\n\")\n",
    "\n",
    "# ========== CORRELATION ANALYSIS ==========\n",
    "corr_matrix = numeric_df.corr()\n",
    "\n",
    "print(\"===== CORRELATION MATRIX =====\")\n",
    "print(corr_matrix)\n",
    "\n",
    "# ========== VISUALIZE CORRELATION ==========\n",
    "plt.figure(figsize=(6,4))\n",
    "sns.heatmap(corr_matrix, annot=True, cmap='coolwarm', fmt=\".2f\")\n",
    "plt.title(\"Correlation Heatmap\")\n",
    "plt.show()\n",
    "\n",
    "# Sort correlations with target\n",
    "corr_with_target = corr_matrix['target'].sort_values(ascending=False)\n",
    "print(\"\\n===== CORRELATION WITH TARGET =====\")\n",
    "print(corr_with_target)\n",
    "\n",
    "# Identify top 3 features most related to target\n",
    "top3_features = corr_with_target[1:4]\n",
    "print(\"\\nTop 3 features most related to target:\")\n",
    "print(top3_features)\n"
   ]
  },
